{
  "openapi": "3.0.0",
  "info": {
    "title": "Joey Example API",
    "description": "A simple API",
    "version": "1.0.0"
  },
  "paths": {
    "/health": {
      "get": {
        "summary": "Health Check",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "OK"
                }
              }
            }
          }
        }
      }
    },
    "/get": {
      "get": {
        "summary": "Get Data",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Data: your_data_here"
                }
              }
            }
          }
        }
      }
    },
    "/post": {
      "post": {
        "summary": "Post Data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "string"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Data received and stored"
                }
              }
            }
          }
        }
      }
    }
  }
}